v>\r\n        </div>\r\n        <!--商品的购买区域-->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-header\">{{goodsInfo.lname}}</div>\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    <p>\r\n                        市场价:<del>¥{{goodsInfo.price_old}}</del>\r\n                        销售价:<span class=\"now_price\">¥{{goodsInfo.price_new}}</span>\r\n                    </p>\r\n                    <p>\r\n                        <div class=\"mui-numbox\">\r\n                            <button class=\"mui-btn mui-btn-numbox-minus\" type=\"button\" @click=\"reduceCount\">-</button>\r\n                            <input class=\"mui-input-numbox\" type=\"number\" :value=\"count\">\r\n                            <button class=\"mui-btn mui-btn-numbox-plus\" type=\"button\" @click=\"increaceCount\">+</button>\r\n\t\t\t\t        </div>\r\n                    </p>\r\n                    <p>\r\n                        <mt-button type=\"primary\" size=\"small\">立即购买</mt-button>\r\n                        <mt-button type=\"danger\" size=\"small\" @click=\"addCart\">加入购物车</mt-button>\r\n                    </p>\r\n                    \r\n                </div>\r\n            </div>\r\n            <div class=\"mui-card-footer\">页脚</div>\r\n        </div>\r\n        <!--商品的参数区域-->\r\n        <div class=\"mui-card\">\r\n            <div class=\"mui-card-header\">页眉</div>\r\n            <div class=\"mui-card-content\">\r\n                <div class=\"mui-card-content-inner\">\r\n                    参数区域\r\n                </div>\r\n            </div>\r\n            <div class=\"mui-card-footer\">页脚</div>\r\n        </div>\r\n    </div>\r\n</template>\r\n<script>\r\n    \r\n    //导入图片轮播组件\r\n    \r\n    import swiper from \"../subcomments/swiper.vue\"\r\n    export default {\r\n        data(){\r\n            return {\r\n                imageList:[],\r\n                id:this.$route.params.id,\r\n                goodsInfo:{},\r\n                count:0\r\n            }\r\n        },\r\n        created(){\r\n            this.getImg();\r\n            this.getGoodsInfo();\r\n        },\r\n        methods:{\r\n            getImg(){\r\n                this.$http.get(\"imagelist/list\").then(result=>{\r\n                    if(result.body.code==1){\r\n                        console.log(result)\r\n                        this.imageList=result.body.msg;\r\n                    }\r\n                })\r\n            },\r\n            getGoodsInfo(){\r\n                console.log(this.id)\r\n                var id=this.id;\r\n                this.$http.get(\"goodslist/find?lid=\"+id).then(result=>{\r\n                    console.log(result);\r\n                    if(result.body.code == 1){\r\n                        this.goodsInfo=result.body.msg[0]\r\n                    }\r\n                })\r\n            },\r\n            increaceCount(){\r\n                this.count++;\r\n            },\r\n            reduceCount(){\r\n                if(this.count>0){\r\n                    this.count--;\r\n                }\r\n                \r\n            },\r\n            addCart(){\r\n                //调用vue实例对象中操作共享数据的方法\r\n                this.$store.commit('increment');\r\n            },\r\n        },\r\n        components:{\r\n            \"swiper-box\":swiper\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n    .goods_info{\r\n        background-color:#eee;\r\n        overflow:hidden;\r\n    }\r\n    .goods_info .now_price{\r\n        font-size:16px;\r\n        color:red;\r\n        font-weight:bold;\r\n    }\r\n</style>"]}]}